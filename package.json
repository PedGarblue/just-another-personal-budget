    "@iconify/json": "^2.2.61",
    "@intlify/devtools-if": "9.2.2",
    "@nuxt/content": "2.8.0",
    "@nuxt/test-utils-edge": "3.1.1-rc.0-27911047.7d812db",
    "@nuxtjs/eslint-config": "^12.0.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@pinia/nuxt": "^0.4.9",
    "@vuepic/vue-datepicker": "^4.5.1",
    "@vueuse/core": "^9.13.0",
    "@vueuse/nuxt": "^9.13.0",
    "chart.js": "^4.4.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^2.30.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "js-yaml": "^4.1.0",
    "lint-staged": "^13.2.2",
    "lucide-vue-next": "^0.368.0",
    "nuxt": "3.2.3",
    "nuxt-windicss": "3.0.1",
    "pinia": "^2.0.35",
    "postcss": "8.4.14",
    "postcss-loader": "^7.3.0",
    "prettier": "^2.8.8",
    "radix-vue": "^1.4.2",
    "sass": "1.53.0",
    "sass-loader": "^13.2.2",
    "serve": "^13.0.4",
    "tailwind-merge": "2.2.2",
    "three": "^0.143.0",
    "translate": "^1.4.1",
    "typescript": "4.9.5",
    "unplugin-icons": "^0.14.15",
    "unplugin-vue-components": "^0.22.12",
    "vfile": "6.0.1",
    "vite": "2.9.7",
    "vitest": "^0.28.5",
    "vue": "3.4.15",
    "vue-chartjs": "^5.2.0",
    "vue-i18n": "^9.14.0",
    "vue-tsc": "1.6.4",
    "vue3-easy-data-table": "^1.5.42",
    "webpack": "^5.82.0"
  },
  "lint-staged": {
    "**/*.{js,ts,vue,html}": [
      "pnpm lintfix"
    ]
  }
}